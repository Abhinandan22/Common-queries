import java.util.Scanner;
import java.io.*;
import java.lang.Math;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
      Scanner sc = new Scanner(System.in);
      int x = sc.nextInt();
      int n = sc.nextInt();
        
      long result = 1;  
        
      while(n!=0){
          result*=x;
          --n;
     
      }  
	
    System.out.println(result);
    }
}
